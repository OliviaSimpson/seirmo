[run]
source = seirmo
omit = 
    seirmo/tests/*
    seirmo/version_info.py
    seirmo/apps/simulation_app.py
    seirmo/apps/dash_app.py

[report]
exclude_lines =
    pragma: no cover
    pragma: no linux cover
    pragma: no python 3 cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    except ImportError:
    if False:
    if __name__ == .__main__.: